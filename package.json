{
  "name": "tutuka-challenge",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.21.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "formidable": "^1.2.2",
    "morgan": "~1.9.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "uuid": "^3.4.0",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.12"
  },
  "scripts": {
    "start": "node ./bin/www",
    "server:dev": "nodemon ./bin/www",
    "client": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean-dist": "rm -rf ./build",
    "copy-to-dist": "cp -r ./public/* ./dist/",
    "set-dist": "npm run clean-dist && npm run copy-to-dist",
    "build:prod": "export NODE_ENV=production && npm run clean-dist && npm run build && npm run start",
    "heroku-prebuild": "npm run clean-dist",
    "heroku-postbuild": "npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
